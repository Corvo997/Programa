<body>
  <div class="container area">
    <!-- Canhoto  -->
    <div class="columns grupo" id="inicio">
      <div class="column ">
        <div class="quadro">
          <div class="columns">
            <div class="column ">
            </div>
          </div>
          <div class="columns linha" id="inicio_linha1">
            <div class="column campo" id="inicio_campo1">
              <div class="texto_recibo conteudo_campo" id="inicio_campo">
                RECEBEMOS DE '{{emitente.nome}}' OS PRODUTOS E/OU SERVIÇOS CONSTANTES DA NOTA FISCAL ELETRÔNICA INDICADA ABAIXO. EMISSÃO: {{data_emissao}} - VALOR TOTAL: {{total_nota}} - DESTINATÁRIO: {{destinatario.nome}} - ENDEREÇO: {{destinatario.endereco}}, {{destinatario.numero}}, {{destinatario.bairro}} - {{destinatario.municipio}}/{{destinatario.uf}}
              </div>
            </div>
          </div>
          <div class="columns linha" >
            <div class="column" id="inicio_linha2">
              <div class="tcampo" id="inicio_campo">DATA DE RECEBIMENTO</div>
            </div>
            <div class="column canhoto_assinatura" id="inicio_linha3">
              <div class="tcampo" id="inicio_campo">IDENTIFICAÇÃO E ASSINATURA DO RECEBEDOR</div>
            </div>
          </div>
        </div>
      </div>
      <div class="column canhoto_nr" id="inicio_canhoto">
        <div class="quadro" >
          <div class="columns">
            <div class="column ">
            </div>
          </div>
          <div class="columns  linha" >
            <div class="column  area_canhoto_nr" id="inicio_canhoto_quadro">
              <div class="center">NF-e</div>
              <div class="">Nº {{numero}}</div>
              <div class="">SÉRIE: {{serie}}</div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Cabeçalho  -->
    <div class="columns grupo" id="cabecalho">
      <div class="column ">
        <div class="quadro">
          <div class="columns">
            <div class="column ">
            </div>
          </div>
          <div class="columns linha quadro_cabecalho" id="cabecalho_columns">
            <div class="column  quadro_identificacao" id="cabecalho_identificacao">
              <div class="conteudo_campo" id="inicio_campo">{{emitente.nome}}</div>
              <div class="conteudo_campo" id="inicio_campo">{{emitente.fantasia}}</div>
              <div class="conteudo_campo" id="inicio_campo">{{emitente.endereco}}, {{emitente.numero}}</div>
              <div class="conteudo_campo" id="inicio_campo">{{emitente.complemento}}</div>
              <div class="conteudo_campo" id="inicio_campo">{{emitente.bairro}} - {{emitente.municipio}} / {{emitente.uf}}</div>
              <div class="conteudo_campo" id="inicio_campo">CEP:{{emitente.cep}} - Fone: {{emitente.telefone}}</div>
            </div>
            <div class="column quadro_danfe" id="cabecalho_danfe">
              <div class="center danfe">DANFE</div>
              <div class="center">DOCUMENTO AUXILIAR DA NOTA FISCAL ELETRÔNICA</div>
              <br>
              <span class="numero center">
                <div class="center is-pulled-right operacao">{{operacao}}</div>
                <div>0- ENTRADA</div>
                <div>1- SAÍDA</div>
                <br>
                <div>Nº {{numero}}</div>
                <div>SÉRIE {{serie}}</div>
                <div>FOLHA ?/?</div>
              </span>
            </div>
            <div class="column quadro_codigo_barra " id="column_barra">
              <div class="codigo_barra">barra</div>
              <div class="chave">
                <div class="tcampo">CHAVE DE ACESSO</div>
                {{chave}}
              </div>
              <div class="consulta">
                <br> Consulta de autenticidade no portal nacional da NF-e
                <span style="text-decoration: underline">www.nfe.fazenda.gov.br/portal</span> ou no site da Sefaz Autorizadora
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="columns grupo" id="cabecalho2">
      <div class="column ">
        <div class="quadro">
          <div class="columns">
            <div class="column ">
            </div>
          </div>
          <div class="columns linha">
            <div class="column campo" id="natureza">
              <div class="tcampo" id="natureza_campo">NATUREZA DA OPERAÇÃO</div>
              <span class="conteudo_campo" id="inicio_campo">{{natureza}}</span>
            </div>
            <div class="column protocolo center" id="protocolo">
              <div class="tcampo" id="protocolo_campo">PROTOCOLO DE AUTORIZACAO DE USO </div>
              <span class="conteudo_campo" id="protocolo_conteudo" id="inicio_campo">{{protocolo}} - {{data_protocolo}}</span>
            </div>
          </div>
          <div class="columns linha">
            <div class="column" id="inscricao">
              <div class="tcampo" id="inicio_campo">INSCRIÇÃO ESTADUAL</div>
              <span class="conteudo_campo" id="inicio_campo">{{emitente.ie}}</span>
            </div>
            <div class="column" id="inscricao2">
              <div class="tcampo" id="inicio_campo" >INSCRIÇÃO ESTADUAL DO SUBST. TRIB.</div>
              <span class="conteudo_campo" id="inicio_campo">{{emitente.ie_st}}</span>
            </div>
            <div class="column" id="cnpj">
              <div class="tcampo" id="inicio_campo">C.N.P.J.</div>
              <span class="conteudo_campo" id="inicio_campo">{{emitente.inscricao_nacional}}</span>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- DEstinatário / remetente -->
    <div class="columns grupo" id="destinatario">
      <div class="column ">
        <div class="quadro">
          <div class="columns">
            <div class="column bold">
              DESTINATÁRIO/REMETENTE
            </div>
          </div>
          <div class="columns linha" id="destinatario_linha">
            <div class="column" id="nome">
              <div class="tcampo" id="inicio_campo">NOME/RAZÃO SOCIAL</div>
              <span class="conteudo_campo" id="inicio_campo">{{destinatario.nome}}</span>
            </div>
            <div class="column" id="cnpj2">
              <div class="tcampo" id="inicio_campo">C.N.P.J./C.P.F.</div>
              <span class="conteudo_campo" id="inicio_campo">{{destinatario.inscricao_nacional}}</span>
            </div>
            <div class="column data" id="data">
              <div class="tcampo" id="inicio_campo">DATA DA EMISSÃO</div>
              <span class="conteudo_campo" id="inicio_campo">{{data_emissao}}</span>
            </div>
          </div>
          <div class="columns linha">
            <div class="column" id="endereco">
              <div class="tcampo" id="inicio_campo">ENDEREÇO</div>
              <span class="conteudo_campo" id="inicio_campo">{{destinatario.endereco}}, {{destinatario.numero}}</span>
            </div>
            <div class="column" id="bairro">
              <div class="tcampo" id="inicio_campo">BAIRRO/DISTRITO</div>
              <span class="conteudo_campo" id="inicio_campo">{{destinatario.bairro}}</span>
            </div>
            <div class="column" id="cep">
              <div class="tcampo" id="inicio_campo">CEP</div>
              <span class="conteudo_campo" id="inicio_campo">{{destinatario.cep}}</span>
            </div>
            <div class="column data" id="data2">
              <div class="tcampo" id="inicio_campo">DATA SAÍDA/ENTRADA</div>
              <span class="conteudo_campo" id="inicio_campo">{{data_saida}}</span>
            </div>
          </div>
          <div class="columns linha">
            <div class="column" id="municipio">
              <div class="tcampo" id="inicio_campo">MUNICÍPIO</div>
              <span class="conteudo_campo" id="inicio_campo">{{destinatario.municipio}}</span>
            </div>
            <div class="column" id="fone">
              <div class="tcampo" id="inicio_campo">FONE/FAX</div>
              <span class="conteudo_campo" id="inicio_campo">{{destinatario.telefone}}</span>
            </div>
            <div class="column uf" id="uf">
              <div class="tcampo" id="inicio_campo">UF</div>
              <span class="conteudo_campo" id="inicio_campo">{{destinatario.uf}}</span>
            </div>
            <div class="column" id="estadual">
              <div class="tcampo" id="inicio_campo">INSCRIÇÃO ESTADUAL</div>
              <span class="conteudo_campo" id="inicio_campo">{{destinatario.ie}}</span>
            </div>
            <div class="column data" id="saida">
              <div class="tcampo" id="inicio_campo">HORA DA SAÍDA</div>
              <span class="conteudo_campo" id="inicio_campo">{{hora_saida}}</span>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Faturas  -->
    {{#if duplicatas}}
    <div class="columns grupo" id="faturas">
      <div class="column ">
        <div class="quadro duplicatas">
          <div class="columns">
            <div class="column bold" id="faturas_bold">
              FATURA/DUPLICATAS
            </div>
          </div>
          <div class="columns linha" id="fatura_linha">
            <div class=" duplicata column is-1" id="fatura_duplicata">
              <div class="tcampo">NÚMERO</div>
              <div class="tcampo">VENCIMENTO</div>
              <div class="tcampo">VALOR</div>
            </div>
            <div class="column" id="fatura_column">
              {{#each duplicatas}}
              <div class=" duplicata is-pulled-left" id="duplicata_pulled">
                <div>{{this.numero}}</div>
                <div>{{this.vencimento}}</div>
                <div>{{this.valor}}</div>
              </div>
              {{/each}}
            </div>
          </div>
        </div>
      </div>
    </div>
    {{/if}}
    <!-- Impostos  -->
    <div class="columns grupo" id="impostos">
      <div class="column ">
        <div class="quadro imposto">
          <div class="columns">
            <div class="column bold" id="impostos_bold">
              CÁLCULO DO IMPOSTO
            </div>
          </div>
          <div class="columns linha">
            <div class="column" id="base_calculo">
              <div class="tcampo" id="inicio_campo">BASE DE CÁLCULO DO ICMS</div>
              <div id="inicio_campo">{{base_calculo_icms}}</div>
            </div>
            <div class="column" id="valor_icms">
              <div class="tcampo" id="inicio_campo">VALOR DO ICMS</div>
              <div id="inicio_campo">{{imposto_icms}}</div>
            </div>
            <div class="column" id="base_calculost">
              <div class="tcampo" id="inicio_campo">BASE DE CÁLCULO DO ICMS ST</div>
              <div id="inicio_campo">{{base_calculo_icms_st}}</div>
            </div>
            <div class="column" id="valor_icmsst">
              <div class="tcampo" id="inicio_campo">VALOR DO ICMS ST</div>
              <div id="inicio_campo">{{imposto_icms_st}}</div>
            </div>
            <div class="column" id="valor_totaltrib">
              <div class="tcampo" id="inicio_campo">VALOR TOTAL APROX. TRIB.</div>
              <div id="inicio_campo">{{imposto_tributos}}</div>
            </div>
            <div class="column" id="valor_produtos">
              <div class="tcampo" id="inicio_campo">VALOR TOTAL DOS PRODUTOS</div>
              <div id="inicio_campo">{{total_produtos}}</div>
            </div>
          </div>
          <div class="columns linha">
            <div class="column" id="valor_frete">
              <div class="tcampo" id="inicio_campo">VALOR DO FRETE</div>
              <div id="inicio_campo">{{total_frete}}</div>
            </div>
            <div class="column" id="valor_seguro">
              <div class="tcampo" id="inicio_campo">VALOR DO SEGURO</div>
              <div id="inicio_campo">{{total_seguro}}</div>
            </div>
            <div class="column" id="desconto">
              <div class="tcampo" id="inicio_campo">DESCONTO</div>
              <div id="inicio_campo">{{total_desconto}}</div>
            </div>
            <div class="column" id="despesas">
              <div class="tcampo" id="inicio_campo">OUTRAS DESPESAS ACESSÓRIAS</div>
              <div id="inicio_campo">{{total_despesas}}</div>
            </div>
            <div class="column" id="ipi">
              <div class="tcampo" id="inicio_campo">VALOR DO IPI</div>
              <div id="inicio_campo">{{total_ipi}}</div>
            </div>
            <div class="column" id="valor_totalnota">
              <div class="tcampo" id="inicio_campo">VALOR TOTAL DA NOTA</div>
              <div id="inicio_campo">{{total_nota}}</div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- transportadora -->
    <div class="columns grupo" id="transportadora">
      <div class="column ">
        <div class="quadro">
          <div class="columns">
            <div class="column bold" id="transportadora_bold">
              TRANSPORTADOR/VOLUMES TRANSPORTADOS
            </div>
          </div>
          <div class="columns linha">
            <div class="column nome" id="transportadora_nome">
              <div class="tcampo" id="inicio_campo">RAZÃO SOCIAL</div>
              <span class="conteudo_campo" id="inicio_campo">{{transportador.nome}}</span>
            </div>
            <div class="column" id="transportadora_frete">
              <div class="tcampo" id="inicio_campo">FRETE POR CONTA DO</div>
              <span class="conteudo_campo" id="inicio_campo">{{modalidade_frete}} - {{modalidade_frete_texto}}</span>
            </div>
            <div class="column" id="transportadora_antt">
              <div class="tcampo" id="inicio_campo">CÓDIGO ANTT</div>
              <span class="conteudo_campo" id="inicio_campo">{{veiculo_antt}}</span>
            </div>
            <div class="column placa" id="transportadora_placa">
              <div class="tcampo" id="inicio_campo">PLACA DO VEÍCULO</div>
              <span class="conteudo_campo" id="inicio_campo">{{veiculo_placa}}</span>
            </div>
            <div class="column uf" id="transportadora_uf">
              <div class="tcampo" id="inicio_campo">UF</div>
              <span class="conteudo_campo" id="inicio_campo">{{veiculo_placa_uf}}</span>
            </div>
            <div class="column" id="transportadora_cnpj">
              <div class="tcampo" id="inicio_campo">C.N.P.J./C.P.F.</div>
              <span class="conteudo_campo" id="inicio_campo">{{transportador.inscricao_nacional}}</span>
            </div>
          </div>
          <div class="columns linha">
            <div class="column" id="transportadora_endereco">
              <div class="tcampo" id="inicio_campo">ENDEREÇO</div>
              <span class="conteudo_campo" id="inicio_campo">{{transportador.endereco}}, {{transportador.numero}}</span>
            </div>
            <div class="column" id="transportadora_municipio">
              <div class="tcampo" id="inicio_campo">MUNICÍPIO</div>
              <span class="conteudo_campo" id="inicio_campo">{{transportador.municipio}}</span>
            </div>
            <div class="column uf" id="transportadora_uf2">
              <div class="tcampo" id="inicio_campo">UF</div>
              <span class="conteudo_campo" id="inicio_campo">{{transportador.uf}}</span>
            </div>
            <div class="column" id="transportadora_inscricao">
              <div class="tcampo" id="inicio_campo">INSCRIÇÃO ESTADUAL</div>
              <span class="conteudo_campo" id="inicio_campo">{{transportador.ie}}</span>
            </div>
          </div>
          <div class="columns linha">
            <div class="column direita" id="transportadora_quantidade">
              <div class="tcampo" id="inicio_campo">QUANTIDADE</div>
              <span class="conteudo_campo" id="inicio_campo">{{volume_quantidade}}</span>
            </div>
            <div class="column" id="transportadora_especie">
              <div class="tcampo" id="inicio_campo">ESPÉCIE</div>
              <span class="conteudo_campo" id="inicio_campo">{{volume_especie}}</span>
            </div>
            <div class="column" id="transportadora_marca">
              <div class="tcampo" id="inicio_campo">MARCA</div>
              <span class="conteudo_campo" id="inicio_campo">{{volume_marca}}</span>
            </div>
            <div class="column" id="transportadora_numeracao">
              <div class="tcampo" id="inicio_campo">NUMERAÇÃO</div>
              <span class="conteudo_campo" id="inicio_campo">{{volume_numeracao}}</span>
            </div>
            <div class="column direita" id="transportadora_bruto">
              <div class="tcampo" id="inicio_campo">PESO BRUTO</div>
              <span class="conteudo_campo" id="inicio_campo">{{volume_pesoBruto}}</span>
            </div>
            <div class="column direita" id="transportadora_liquido">
              <div class="tcampo" id="inicio_campo">PESO LÍQUIDO</div>
              <span class="conteudo_campo" id="inicio_campo">{{volume_pesoLiquido}}</span>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Itens  -->
    <div class="columns grupo" id="itens">
      <div class="column ">
        <div class="quadro">
          <div class="columns">
            <div class="column bold" id="itens_bold">
              DADOS DOS PRODUTOS/SERVIÇOS
            </div>
          </div>
          <div class="columns">
            <table class="table is-bordered itens">
              <thead>
                <tr>
                  <th>CÓDIGO</th>
                  <th>DESCRIÇÃO</th>
                  <th>NCM/SH</th>
                  <th>CST</th>
                  <th>CFOP</th>
                  <th>UN.</th>
                  <th>QUANT.</th>
                  <th>V.UNIT.</th>
                  <th>V.DESC.</th>
                  <th>V.TOTAL</th>
                  <th>BC.ICMS</th>
                  <th>V.ICMS</th>
                  <th>V.IPI</th>
                  <th>%ICMS</th>
                  <th>%IPI</th>
                </tr>
              </thead>
              <tbody>
                {{#each itens}}
                <tr>
                  <td>{{this.codigo}}</td>
                  <td>{{this.descricao}}</td>
                  <td>{{this.ncm}}</td>
                  <td>{{this.cst}}</td>
                  <td>{{this.cfop}}</td>
                  <td>{{this.unidade}}</td>
                  <td class="direita">{{this.quantidade}}</td>
                  <td class="direita">{{this.valor}}</td>
                  <td class="direita">{{this.desconto}}</td>
                  <td class="direita">{{this.total}}</td>
                  <td class="direita">{{this.base_calculo}}</td>
                  <td class="direita">{{this.icms}}</td>
                  <td class="direita">{{this.ipi}}</td>
                  <td class="direita">{{this.porcentagem_icms}}</td>
                  <td class="direita">{{this.porcentagem_ipi}}</td>
                </tr>
                {{/each}}

              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <!-- Serviço  -->
    {{#if total_servico}}
    <div class="columns grupo" id="servico">
      <div class="column ">
        <div class="quadro">
          <div class="columns">
            <div class="column bold">
              CÁLCULO DO ISSQN
            </div>
          </div>
          <div class="columns linha">
            <div class="column">
              <div class="tcampo">INSCRIÇÃO MUNICIPAL</div>
              <span class="conteudo_campo">{{inscricao_municipal}}</span>
            </div>
            <div class="column">
              <div class="tcampo">VALOR TOTAL DOS SERVIÇOS</div>
              <div class="conteudo_campo direita">{{total_servico}}</div>
            </div>
            <div class="column">
              <div class="tcampo">BASE DE CÁLCULO DO ISSQN</div>
              <div class="conteudo_campo direita">{{base_calculo_issqn}}</div>
            </div>
            <div class="column">
              <div class="tcampo">VALOR DO ISSQN</div>
              <div class="conteudo_campo direita">{{total_issqn}}</div>
            </div>
          </div>
        </div>
      </div>
    </div>
    {{/if}}
    <!-- Dados adicionais  -->
    <div class="columns grupo" id="fim">
      <div class="column ">
        <div class="quadro">
          <div class="columns">
            <div class="column bold" id="fim_bold">
              DADOS ADICIONAIS
            </div>
          </div>
          <div class="columns linha complemento">
            <div class="column" id="fim_complemento">
              <div class="tcampo" id="inicio_campo">INFORMAÇÕES COMPLEMENTARES</div>
              <span class="conteudo_campo" id="inicio_campo">{{informacoes_fisco}}</span>
              <span class="conteudo_campo" id="inicio_campo">{{informacoes_complementares}}</span>
              <span class="conteudo_campo" id="inicio_campo">{{observacao}}</span>
            </div>
            <div class="column fisco" id="fim_fisco">
              <div class="tcampo" id="inicio_campo">RESERVADO AO FISCO</div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>

</html>